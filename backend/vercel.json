{
  "version": 2,
  "installCommand": "npm install && npx prisma generate",
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": ".",
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
